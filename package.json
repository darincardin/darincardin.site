  "scripts": {
    "save": "git add . && git commit -m 'test' && git push",
    "go": "npm run build && npm run serve",
    "build": "preact build && npm run build_config",
    "serve": "sirv build --port 8080 --cors --single",
    "start": "preact watch",
    "lint": "eslint src",
    "build_config": " xcopy /I /E /Y \"config\" \"build/\"",
    "move": "cd .. &&  xcopy /I /E /Y \"jquery-plugins/dist\" \"darincardin.site/node_modules/jquery-plugins/dist\" && cd darincardin.site"
  },

  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-preact-pure": "^2.0.0",
    "jquery-plugins1": "github:darincardin/jquery-plugins",
    "preact-cli": "^3.0.0",
    "react-widgets4": "github:darincardin/react-widgets",
    "sass": "^1.54.8",
    "sass-loader": "^10.2.0",
    "sirv-cli": "1.0.3"
  },
  "dependencies": {
    "bootstrap": "5.2",
    "jquery": "^3.4.1",
    "preact": "^10.3.2",
    "preact-render-to-string": "^5.1.4",
    "preact-router": "^3.2.1",
    "react-bootstrap": "^2.5.0",
    "react-router-dom": "^5.1.2"
  }
